<table>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
    <% @customers.each do |customer| %>
    <tr>
      <td><%= customer.id %></td>
      <td>
        <%= link_to admins_customer_path(customer) do %>
          <%= customer.last_name %> <%= customer.first_name %>
        <% end %>
      </td>
      <td><%= customer.email %></td>
      <td>
        <% if customer.is_deleted == false %>
          有効
        <% else %>
          退会
        <% end %>
      </td>
    </tr>
    <% end %>
  </table>
  