<h3>商品一覧</h3>
  <%= link_to '商品新規登録', new_admins_item_path %>
  <table>
    <tr>
      <td>商品ID</td>
      <td>商品名</td>
      <td>税抜き価格</td>
      <td>ジャンル</td>
      <td>ステータス</td>
    </tr>
    <% @items.each do |item| %>
    <tr>
      <td><%= item.id %></td>
      <td>
        <%= link_to item.name, admins_item_path(item) %>
      </td>
      <td><%= item.price %></td>
      <td><%= item.genre.name %></td>
      <td>
        <% if item.sell_status == true %>
          販売中
        <% else %>
          販売停止中
        <% end %>
      </td>
    </tr>
    <% end %>
  </table>