<div class="container">
  <h3>配送先編集</h3>
  
  <%= form_with model: @address, local:true do |f| %>
    <div class="form-group">
      <%= f.label :"郵便番号(ハイフンなし)" %>
      <%= f.text_field :postal_code %>
    </div>
    <div class="form-group">
      <%= f.label :"住所" %>
      <%= f.text_field :address %>
    </div>
    <div class="form-group">
      <%= f.label :"宛名" %>
      <%= f.text_field :name %>
    </div>
    <div class="form-group">
      <%= f.submit "変更内容を保存", class: "btn btn-success" %>
    </div>
  <% end %>
</div>

      <table class="address">
        <tr class="address">
          <td class="address"><%= f.label :"郵便番号(ハイフンなし)" %></td>
          <td class="address"><%= f.text_field :postal_code %></td>
          <td class="address"></td>
          <td class="address"></td>
        </tr>
        <tr class="address">
          <td class="address"><%= f.label :"住所" %></td>
          <td class="address"><%= f.text_field :address, size:'50' %></td>
          <td class="address"></td>
        </tr>
        <tr class="address">
          <td class="address"><%= f.label :"宛名" %></td>
          <td class="address"><%= f.text_field :name %></td>
          <td class="address"></td>
          <td class="address"><%= f.submit "新規登録", class: "btn btn-success" %></td>
        </tr>
      </table>